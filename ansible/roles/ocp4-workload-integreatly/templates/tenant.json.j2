apiVersion: capabilities.3scale.net/v1alpha1
kind: Tenant
metadata:
  name: {{ ocp4_workload_integreatly_defaults.tenant_name }}
spec:
  email: {{ ocp4_workload_integreatly_defaults.tenant_admin_email }}
  username: {{ ocp4_workload_integreatly_defaults.tenant_admin_user }}
  passwordCredentialsRef:
    name: {{ tenant_admin_secret }}
  masterCredentialsRef:
    name: {{ ocp4_workload_integreatly_defaults.seed_secret_name }}
  organizationName: {{ ocp4_workload_integreatly_defaults.tenant_name }}
  systemMasterUrl: {{ threescale_master_host }}
  tenantSecretRef:
    name: {{ ocp4_workload_integreatly_defaults.tenant_detail_secret_name }}
    namespace: {{ ocp4_workload_integreatly_defaults.tenant_detail_secret_namespace }} 